{
  "name": "voxelviewer",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "corepack pnpm -r run build",
    "test": "corepack pnpm -r run build && corepack pnpm -r run test",
    "typecheck": "corepack pnpm -r run typecheck",
    "lint": "corepack pnpm -r run lint",
    "indexer": "corepack pnpm --filter @voxelviewer/indexer run cli --",
    "desktop": "tsx scripts/desktop-open.ts",
    "desktop:dev": "corepack pnpm --filter @voxelviewer/desktop run dev",
    "desktop:open": "tsx scripts/desktop-open.ts",
    "smoke": "tsx scripts/smoke-test.ts",
    "test:compat": "tsx scripts/compatibility-corpus.ts",
    "test:compat:update": "tsx scripts/compatibility-corpus.ts --update",
    "test:ui": "tsx scripts/playwright-ui-smoke.ts"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.3.0",
    "@types/pngjs": "^6.0.5",
    "@types/three": "^0.179.0",
    "fast-check": "^4.2.0",
    "playwright": "^1.55.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "electron",
      "esbuild"
    ]
  }
}
